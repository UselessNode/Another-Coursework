<Window x:Class="Praxix.View.AuthorizationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praxix.View"
        mc:Ignorable="d"
        Title="AuthorizationWindow" Height="100" Width="200">
    <Grid>
        <StackPanel>
            <DockPanel>
                <Label Content="Логин" Width="50"/>
                <TextBox/>
            </DockPanel>
            <DockPanel>
                <Label Content="Пароль" Width="50"/>
                <TextBox/>
            </DockPanel>
            <Button Width="60" Content="Вход"/>
        </StackPanel>
    </Grid>
</Window>
